class Vehicle
	init
		@wheels = 4
		@color = "black"

	init_with_color:color
		@wheels = 4
		@color = color

	init_with_wheel_number:wheels, color:color
		@wheels = wheels
		@color = color

	get_color
		return @color

car = [Vehicle, init_with_wheel_number:6, color:"red"]
car_color = [car, get_color]
